{
	"name": "adworks_ChannelType",
	"properties": {
		"folder": {
			"name": "adworkstarget"
		},
		"type": "MappingDataFlow",
		"typeProperties": {
			"sources": [
				{
					"name": "ChannelTypeSource"
				}
			],
			"sinks": [
				{
					"name": "ChannelType"
				}
			],
			"transformations": [
				{
					"name": "SurrogateKey"
				}
			],
			"scriptLines": [
				"source(output(",
				"          ChannelTypeId as short,",
				"          ChannelTypeName as string",
				"     ),",
				"     allowSchemaDrift: true,",
				"     validateSchema: false,",
				"     store: 'synapse',",
				"     databaseType: 'spark',",
				"     format: 'table',",
				"     database: 'adworks',",
				"     tableName: 'ChannelType') ~> ChannelTypeSource",
				"ChannelTypeSource keyGenerate(output(ChannelTypeKey as long),",
				"     startAt: 1L,",
				"     stepValue: 1L) ~> SurrogateKey",
				"SurrogateKey sink(allowSchemaDrift: true,",
				"     validateSchema: false,",
				"     store: 'synapse',",
				"     databaseType: 'spark',",
				"     format: 'table',",
				"     database: 'adworkstarget',",
				"     tableName: 'ChannelType',",
				"     recreate:true,",
				"     mapColumn(",
				"          ChannelTypeId,",
				"          ChannelTypeName,",
				"          ChannelTypeKey",
				"     )) ~> ChannelType"
			]
		}
	}
}