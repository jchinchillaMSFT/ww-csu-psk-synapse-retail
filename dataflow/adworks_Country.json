{
	"name": "adworks_Country",
	"properties": {
		"folder": {
			"name": "AdventureWorksDataFlows"
		},
		"type": "MappingDataFlow",
		"typeProperties": {
			"sources": [
				{
					"name": "CountrySource"
				}
			],
			"sinks": [
				{
					"name": "Country"
				}
			],
			"transformations": [],
			"scriptLines": [
				"source(output(",
				"          CountryId as short,",
				"          IsoCountryName as string",
				"     ),",
				"     allowSchemaDrift: true,",
				"     validateSchema: false,",
				"     store: 'synapse',",
				"     databaseType: 'spark',",
				"     format: 'table',",
				"     database: 'adworks',",
				"     tableName: 'Country') ~> CountrySource",
				"CountrySource sink(allowSchemaDrift: true,",
				"     validateSchema: false,",
				"     store: 'synapse',",
				"     databaseType: 'spark',",
				"     format: 'table',",
				"     database: 'adworkstarget',",
				"     tableName: 'Country',",
				"     recreate:true) ~> Country"
			]
		}
	}
}