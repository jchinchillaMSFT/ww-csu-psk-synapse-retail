{
	"name": "GithubFiles",
	"properties": {
		"parameters": {
			"githubAccount": {
				"type": "string"
			},
			"projectName": {
				"type": "string"
			},
			"branch": {
				"type": "string",
				"defaultValue": "main"
			}
		},
		"annotations": [],
		"type": "HttpServer",
		"typeProperties": {
			"url": "@{concat('https://raw.githubusercontent.com/',linkedService().githubAccount,'/',linkedService().projectName,'/',linkedService().branch)}",
			"enableServerCertificateValidation": true,
			"authenticationType": "Anonymous"
		},
		"connectVia": {
			"referenceName": "AutoResolveIntegrationRuntime",
			"type": "IntegrationRuntimeReference"
		}
	}
}